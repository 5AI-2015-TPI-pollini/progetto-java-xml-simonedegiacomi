<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="weatherproject.PreferencesController">
   <children>
      <VBox layoutX="10.0" layoutY="10.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <GridPane>
                     <children>
                        <Label text="Use Proxy">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </Label>
                        <Label text="Server IP" GridPane.rowIndex="1">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </Label>
                        <Label text="Server Port" GridPane.rowIndex="2">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </Label>
                        <CheckBox fx:id="proxyCheck" mnemonicParsing="false" GridPane.columnIndex="1">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </CheckBox>
                        <TextField fx:id="proxyIP" disable="true" promptText="192.168.0.1" GridPane.columnIndex="1" GridPane.rowIndex="1">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </TextField>
                        <TextField fx:id="proxyPort" disable="true" promptText="8080" GridPane.columnIndex="1" GridPane.rowIndex="2">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                           <GridPane.margin>
                              <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                           </GridPane.margin>
                        </TextField>
                        <Label text="Authentication" GridPane.rowIndex="3" />
                        <Label text="User" GridPane.rowIndex="4" />
                        <Label text="Password" GridPane.rowIndex="5" />
                        <TextField disable="true" promptText="Username" GridPane.columnIndex="1" GridPane.rowIndex="4">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding></TextField>
                        <TextField disable="true" promptText="Password" GridPane.columnIndex="1" GridPane.rowIndex="5">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding></TextField>
                        <CheckBox fx:id="authenticateProxy" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding></CheckBox>
                     </children>
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                     </columnConstraints>
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                     </rowConstraints>
                  </GridPane>
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </Pane>
            <Pane>
               <children>
                  <HBox prefHeight="100.0" prefWidth="244.0">
                     <children>
                        <Label text="Data Type">
                           <padding>
                              <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                           </padding>
                        </Label>
                        <ChoiceBox prefWidth="150.0" />
                     </children>
                  </HBox>
               </children>
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </VBox.margin>
            </Pane>
            <Pane prefHeight="60.0" prefWidth="260.0">
               <children>
                  <Button fx:id="save" layoutX="194.0" layoutY="2.0" mnemonicParsing="false" text="Ok" />
               </children>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
